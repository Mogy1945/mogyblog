{
  "root": true,
  "env": {
    "es2021": true
  },
  "parserOptions": {
		"ecmaVersion": 13
	},
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:@next/next/recommended"
  ],
  "rules": {
    "no-console": "off"
  },
  "ignorePatterns": [
    "/node_modules/",
    "/dist/"
  ]
}